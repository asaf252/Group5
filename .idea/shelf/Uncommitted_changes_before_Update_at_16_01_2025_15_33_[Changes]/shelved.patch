+/* General Layout Adjustments */
+.MainStyle {
+    display: flex;
+    flex-direction: column;
+    align-items: center;
+    padding: 20px;
+    background-color: #fff7f0;
+    border-radius: 15px;
+    box-shadow: 0 4px 10px rgba(0, 0, 0, 0.2);
+    max-width: 1000px;
+    margin: auto;
+}
+
+.MainGrid {
+    display: grid;
+    grid-template-columns: 1fr 1fr; /* Two equal columns for content and image */
+    grid-gap: 20px;
+    width: 100%;
+    margin-top: 20px;
+}
+
+.left-section {
+    padding: 20px;
+    display: flex;
+    flex-direction: column;
+}
+
+.rating {
+    margin-bottom: 20px;
+}
+
+.rating-stars img {
+    width: 40px;
+    height: 40px;
+    margin-right: 5px;
+    transition: transform 0.2s ease;
+}
+
+.rating-stars img:hover {
+    transform: scale(1.1);
+}
+
+.about-section {
+    margin-bottom: 20px;
+}
+
+.info-box {
+    background-color: #fdf6f3;
+    padding: 10px;
+    border-radius: 8px;
+    margin-top: 10px;
+}
+
+.unified-box {
+    display: flex;
+    flex-wrap: wrap;
+    justify-content: space-between;
+    margin-top: 20px;
+}
+
+.left-column, .right-column {
+    width: 45%;
+    margin-bottom: 10px;
+}
+
+.right-section {
+    display: flex;
+    justify-content: center;
+    align-items: center;
+}
+
+.dog-picture {
+    width: 100%;
+    max-width: 400px;
+    border-radius: 10px;
+    object-fit: cover;
+}
+
+/* Footer Buttons */
+.Raw {
+    display: flex;
+    justify-content: center;
+    gap: 20px;
+}
+
+.btmType1, .btmType2 {
+    background-color: #7b4d3a;
+    color: white;
+    padding: 10px 20px;
+    font-size: 16px;
+    border-radius: 5px;
+    border: none;
+    cursor: pointer;
+    transition: background-color 0.3s;
+}
+
+.btmType1:hover, .btmType2:hover {
+    background-color: #f3d8c7;
+}
+
+/* Responsive Design for Small Screens */
+@media (max-width: 768px) {
+    .MainGrid {
+        grid-template-columns: 1fr; /* Single column layout on small screens */
+    }
+
+    .right-section {
+        margin-top: 20px; /* Move the image below the text on small screens */
+    }
+
+    .dog-picture {
+        max-width: 100%; /* Make sure image takes full width on mobile */
+    }
+}
