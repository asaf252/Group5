<+>/* General Layout Adjustments */\r\n.MainStyle {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    padding: 20px;\r\n    background-color: #fff7f0;\r\n    border-radius: 15px;\r\n    box-shadow: 0 4px 10px rgba(0, 0, 0, 0.2);\r\n    max-width: 1000px;\r\n    margin: auto;\r\n}\r\n\r\n.MainGrid {\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr; /* Two equal columns for content and image */\r\n    grid-gap: 20px;\r\n    width: 100%;\r\n    margin-top: 20px;\r\n}\r\n\r\n.left-section {\r\n    padding: 20px;\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n\r\n.rating {\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.rating-stars img {\r\n    width: 40px;\r\n    height: 40px;\r\n    margin-right: 5px;\r\n    transition: transform 0.2s ease;\r\n}\r\n\r\n.rating-stars img:hover {\r\n    transform: scale(1.1);\r\n}\r\n\r\n.about-section {\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.info-box {\r\n    background-color: #fdf6f3;\r\n    padding: 10px;\r\n    border-radius: 8px;\r\n    margin-top: 10px;\r\n}\r\n\r\n.unified-box {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: space-between;\r\n    margin-top: 20px;\r\n}\r\n\r\n.left-column, .right-column {\r\n    width: 45%;\r\n    margin-bottom: 10px;\r\n}\r\n\r\n.right-section {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n\r\n.dog-picture {\r\n    width: 100%;\r\n    max-width: 400px;\r\n    border-radius: 10px;\r\n    object-fit: cover;\r\n}\r\n\r\n/* Footer Buttons */\r\n.Raw {\r\n    display: flex;\r\n    justify-content: center;\r\n    gap: 20px;\r\n}\r\n\r\n.btmType1, .btmType2 {\r\n    background-color: #7b4d3a;\r\n    color: white;\r\n    padding: 10px 20px;\r\n    font-size: 16px;\r\n    border-radius: 5px;\r\n    border: none;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s;\r\n}\r\n\r\n.btmType1:hover, .btmType2:hover {\r\n    background-color: #f3d8c7;\r\n}\r\n\r\n/* Responsive Design for Small Screens */\r\n@media (max-width: 768px) {\r\n    .MainGrid {\r\n        grid-template-columns: 1fr; /* Single column layout on small screens */\r\n    }\r\n\r\n    .right-section {\r\n        margin-top: 20px; /* Move the image below the text on small screens */\r\n    }\r\n\r\n    .dog-picture {\r\n        max-width: 100%; /* Make sure image takes full width on mobile */\r\n    }\r\n}\r\n
