+/* מבנה העמוד */
+.kennel-container {
+    display: grid;
+    grid-template-columns: 1fr 2fr; /* 1 חלק לתמונה, 2 חלקים למידע */
+    gap: 20px;
+    max-width: 1000px;
+    margin: 20px auto;
+    padding: 20px;
+    background-color: #fff;
+    border-radius: 10px;
+    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
+}
+
+/* תמונה */
+.kennel-image img {
+    width: 100%;
+    height: auto;
+    border-radius: 10px;
+    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
+    object-fit: cover;
+}
+
+/* חלוקה ל-columns במידע */
+.unified-box {
+    display: grid;
+    grid-template-columns: 1fr 1fr; /* שני טורים */
+    gap: 20px;
+}
+
+.unified-box .left-column,
+.unified-box .right-column {
+    display: flex;
+    flex-direction: column;
+    gap: 10px; /* ריווח בין הפריטים */
+}
+
+.left-column p,
+.right-column p {
+    margin: 0;
+}
+
+/* דירוג (Grade) */
+.rating {
+    display: flex;
+    align-items: center;
+    gap: 10px; /* ריווח בין התווית לעצמות */
+}
+
+.rating-stars img {
+    width: 20px;
+    height: auto;
+}
+
+/* טקסט מרכזי */
+.about-section {
+    margin-top: 20px;
+}
+
+.info-box {
+    font-size: 16px;
+    line-height: 1.5;
+    color: #333;
+    padding: 10px;
+    border: 1px solid #ddd;
+    border-radius: 5px;
+    background-color: #f9f9f9;
+}
+
+/* טקסטים בולטים */
+label {
+    font-weight: bold;
+    color: #6b4226;
+}
